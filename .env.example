# Node Environment
NODE_ENV=development

# Service Ports
ROBOTICS_HTTP_PORT=9113
ROBOTICS_WS_PORT=9114

# Integration Service URLs
VIDEO_AGENT_URL=http://videoagent-api:9200
GEO_AGENT_URL=http://nexus-geoagent-api:9103
GEO_AGENT_ML_URL=http://nexus-geoagent-ml:9108
SPECTRUM_AGENT_URL=http://spectrum-api:8092
MAGE_AGENT_URL=http://nexus-mageagent:9080
ORCHESTRATION_URL=http://nexus-orchestrationagent:9109
GRAPHRAG_URL=http://nexus-graphrag:9090
GATEWAY_URL=http://nexus-gateway:9092

# Database
POSTGRES_HOST=postgres
POSTGRES_PORT=9432
POSTGRES_DB=nexus_robotics
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password_here

# Redis
REDIS_HOST=redis
REDIS_PORT=9379
REDIS_PASSWORD=

# Feature Flags
ENABLE_GPS_DENIED_NAV=true
ENABLE_AUTONOMOUS_ENGAGEMENT=false
ENABLE_SWARM_COORDINATION=true
ENABLE_EW_COUNTERMEASURES=true
ENABLE_STARLINK_C2=true
ENABLE_TELEMETRY_UPLOAD=true
ENABLE_MISSION_LEARNING=true

# CORS
CORS_ORIGIN=*

# Logging
LOG_LEVEL=info
